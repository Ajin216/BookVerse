            <div class="container-fluid">
                <div class="card order-card">
                    <div class="card-header">
                        <h2 class="mb-0">Order Details</h2>
                        <span class="badge">#<%= order.orderId %></span>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="order-info-section">
                                    <h4 class="mb-3">Shipping Address</h4>
                                    <p class="mb-1"><strong><%= address.addressName %></strong></p>
                                    <p class="mb-1"><%= address.addressHouse %></p>
                                    <p class="mb-1"><%= address.addressPost %>, <%= address.addressDistrict %></p>
                                    <p class="mb-1"><%= address.addressState %> - <%= address.addressPin %></p>
                                    <p class="mb-1"><i class="bi bi-telephone me-2"></i>Contact: <%= address.addressMobile %></p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="order-info-section">
                                    <h4 class="mb-3">Order Information</h4>
                                    <p class="mb-1"><strong>Order Date:</strong> <%= order.createdAt.toLocaleDateString() %></p>
                                    <p class="mb-1"><strong>Payment Method:</strong> <%= order.paymentMethod %></p>
                                    <p class="mb-1">
                                        <strong>Order Status:</strong> 
                                        <span class="badge status-badge <%= 
                                            order.order_status === 'Delivered' ? 'bg-success' :
                                            order.order_status === 'Processing' ? 'bg-warning' :
                                            order.order_status === 'Cancelled' ? 'bg-danger' :
                                            'bg-secondary'
                                        %>">
                                            <%= order.order_status %>
                                        </span>
                                    </p>
                                </div>
                            </div>
                        </div>
            
                        <div class="table-responsive mt-4">
                            <table class="table table-custom table-striped">
                                <thead>
                                    <tr>
                                        <th>SI No.</th>
                                        <th>Product Name</th>
                                        <th>Category</th>
                                        <th>Quantity</th>
                                        <th>Price</th>
                                        <th>Coupon Discount</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% items.forEach((item, index) => { %>
                                    <tr>
                                        <td><%= index + 1 %></td>
                                        <td><%= item.productId.productName %></td>
                                        <td><%= item.productId.category.name %></td>
                                        <td><%= item.quantity %></td>
                                        <td>₹<%= item.price.toFixed(2) %></td>
                                        <td>₹<%= item.total.toFixed(2) %></td>
                                    </tr>
                                    <% }); %>
                                </tbody>
                                <tfoot>
                                    <% 
                                    const subtotalAmount = items.reduce((sum, item) => sum + item.total, 0);
                                    const discountAmount = subtotalAmount - order.totalPrice;
                                    %>
                                    <tr>
                                        <td colspan="5" class="text-end"><strong>Subtotal:</strong></td>
                                        <td>₹<%= subtotalAmount.toFixed(2) %></td>
                                    </tr>
                                    <% if (discountAmount > 0) { %>
                                    <tr>
                                        <td colspan="5" class="text-end text-danger"><strong>Coupon Discount:</strong></td>
                                        <td class="text-danger">-₹<%= discountAmount.toFixed(2) %></td>
                                    </tr>
                                    <% } %>
                                    <tr class="summary-total">
                                        <td colspan="5" class="text-end"><strong>Total:</strong></td>
                                        <td><strong>₹<%= order.totalPrice.toFixed(2) %></strong></td>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
            
                        <div class="text-center mt-4">
                            <a href="/" class="btn btn-outline-secondary btn-custom me-2">Back to Home</a>
                            <a href="/order" class="btn btn-danger btn-custom me-2">Order List</a>
                            <a href="/order/<%= order.orderId %>/invoice" class="btn btn-primary btn-custom">
                                <i class="bi bi-file-earmark-pdf me-1"></i> Download Invoice
                            </a>
                        </div>
